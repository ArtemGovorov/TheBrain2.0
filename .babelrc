{
  "plugins": [
    "transform-async-to-generator",
    "transform-react-jsx"
  ],
  "presets": [
    "es2015",
    "stage-2",
    "flow"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "stage-2",
        "flow"
      ],
      "plugins": [
        [
          "module-rewrite-with-root",
          {
            "replaceFunc": "./testing/testHelpers/replace-for-testing.js"
          }
        ]
      ]
    },
    "development": {
      "presets": [
        "es2015",
        "stage-2",
        "flow"
      ],
      "plugins": [
        [
          "module-rewrite-with-root",
          {
            "replaceFunc": "./testing/testHelpers/replace-for-testing.js"
          }
        ],
        "get-rid-of-async-await"
      ]
    }
  }
}